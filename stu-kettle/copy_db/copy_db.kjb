      <description/>
      <type>EVAL</type>
      <attributes/>
      <script>var tables=parent_job.getVariable("tab_names").replace("[","").replace("]","").split(",");
var current=parent_job.getVariable("current_tab");
var index=0;
while(true){
	if(current==tables[index].trim()){
		break;
	}
	index++;
}
if(index==tables.length){
	false;
}else{
	parent_job.setVariable("current_tab",tables[index+1].trim());
	true;
}



</script>
